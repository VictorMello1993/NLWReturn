generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id String @id @default(uuid()) //PK
  type String
  comment String
  screenshot String?

  @@map("feedbacks") //Nome da tabela no SQL nativo ser√° "feedbacks"
}
